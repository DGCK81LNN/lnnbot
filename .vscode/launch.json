{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "type": "pwa-node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "Build",
            "outFiles": [
                "${workspaceFolder}/bin/**/*.js"
            ],
            "program": "${workspaceFolder}\\bin\\index.js",
            "outputCapture": "std"
        }
    ]
}